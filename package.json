{
  "name": "vscode-cosmo",
  "publisher": "cosmo",
  "displayName": "Cosmo Language",
  "description": "Provides syntax highlighting and code snippets for the Cosmo programming language made by R-unic.",
  "version": "0.10.1",
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.7.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "url": "https://github.com/R-unic/vscode-cosmo"
  },
  "contributes": {
    "languages": [
      {
        "id": "cosmo",
        "aliases": [
          "Cosmo",
          "cosmo"
        ],
        "extensions": [
          ".cos",
          ".‚≠ê"
        ],
        "icon": {
          "light": "./assets/icon.png",
          "dark": "./assets/icon.png"
        },
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "cosmo",
        "scopeName": "source.cosmo",
        "path": "./syntaxes/cosmo.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "cosmo",
        "path": "./snippets/cosmo.json"
      }
    ]
  }
}
